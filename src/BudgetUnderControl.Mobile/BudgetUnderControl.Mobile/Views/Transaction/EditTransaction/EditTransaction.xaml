<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BudgetUnderControl.Views.EditTransaction"
             xmlns:local="clr-namespace:BudgetUnderControl.Views;assembly=BudgetUnderControl.Mobile"
             xmlns:behaviors="clr-namespace:BudgetUnderControl.Behaviors;assembly=BudgetUnderControl.Mobile"
             >
    <TabbedPage.Children>
        <local:EditTransactionOverview  Title="Overview" x:Name="editTransactionOverview"  BindingContext="{Binding vm}" />
        <local:EditTransactionExtra  Title="Extra" x:Name="editTransactionExtra" BindingContext="{Binding vm}"/>
        <local:EditTransactionFile Title="File" x:Name="editTransactionFile" BindingContext="{Binding vm}"/>
    </TabbedPage.Children>
</TabbedPage>

