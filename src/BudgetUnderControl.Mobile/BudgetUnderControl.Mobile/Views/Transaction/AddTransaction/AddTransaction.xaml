<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BudgetUnderControl.Views.AddTransaction"
             xmlns:local="clr-namespace:BudgetUnderControl.Views;assembly=BudgetUnderControl.Mobile"
             xmlns:behaviors="clr-namespace:BudgetUnderControl.Behaviors;assembly=BudgetUnderControl.Mobile"
             >
    <TabbedPage.Children>
        <local:AddTransactionOverview  Title="Overview" x:Name="addTransactionOverview"  BindingContext="{Binding vm}"/>
        <local:AddTransactionExtra  Title="Extra" x:Name="addTransactionExtra" BindingContext="{Binding vm}"/>
        <local:AddTransactionFile Title="File" x:Name="addTransactionFile" BindingContext="{Binding vm}"/>
    </TabbedPage.Children>
    </TabbedPage>